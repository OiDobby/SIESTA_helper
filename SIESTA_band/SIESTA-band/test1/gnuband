set term postscript eps color enhanced
set output "Si-band.eps"
set nokey
set yr [-4:4]
set xtic ("{/Symbol G}"0, "K"0.745356, "M"1.118034, "{/Symbol G}"1.618034)
set mytic 2
set ytic 1
set grid xtics lw 4
set xzeroaxis lw 4
set xtic font "Times Roman,30"
set ytic font "Times Roman,30"
set tics font "Times Roman,30"
set tmargin 3
set bmargin 6
set lmargin 10
set rmargin 2
set xlabel 'Wave Vector' font "Times Roman, 36" offset 0,-1.5,0
set ylabel 'E-E_F (eV)' font "Times Roman, 36" offset -2,0,0
plot    "up-band_siesta" using ($1):($2+3.574050) w l  lw 4 lt 1 lc 0 ,\
